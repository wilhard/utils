<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="ctd.account.user.User" table="BASE_User">
			<id name="id" type="java.lang.String">
            	<column name="id"/>
            	<generator class="assigned" />
        	</id>
        	<property name="name" type="java.lang.String" length="50" />
        	<property name="password" type="java.lang.String" length="32" />
        	<property name="status" type="java.lang.String" length="1" />
        	<property name="createDt" type="java.util.Date"  />
        	<property name="pageCount" type="java.lang.Integer"  />
</class>
</hibernate-mapping>